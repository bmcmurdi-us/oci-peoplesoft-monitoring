        <DBPattern dateTimeField="entrytime" dateTimeColumns="entrytime">
        <SQLQuery>
            <![CDATA[select 'PSFT_CS_FA_AID_YR_MISMATCH' sensor, to_timestamp(to_char(sysdate, 'YYYY-MM-DD hh24:mi:ss'), 'YYYY-MM-DD hh24:mi:ss') entrytime, s.KEY_DATA FROM (Select 'Review' ||count(*)|| ' is not set up properly or not exist in the valid term(s) for career (See KM 2406742.1 )' AS KEY_DATA,  'Please refer to Doc ID 2406742.1 to learn the possible causes of this issue and how to solve it' AS SOLUTION,  'Y' AS EVENT  From  (Select distinct T.emplid, T.institution, T.strm, T.acad_career, T.aid_year  from SYSADM.ps_stdnt_fa_term T  , SYSADM.ps_aid_year_tbl A  where A.acad_year_end >= sysdate  and T.institution = A.institution  and T.aid_year = A.aid_year  and not exists (Select 'X'  From SYSADM.ps_aid_yr_car_term A  where T.institution = A.institution  and T.aid_year = A.aid_year  and T.acad_career = A.acad_career  and T.strm = A.strm))   having count(*) > 0) s]]>
