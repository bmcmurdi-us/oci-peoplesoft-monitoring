    <IsSecureContent>1</IsSecureContent>
    <Patterns>
        <DBPattern dateTimeField="entrytime" dateTimeColumns="entrytime">
        <SQLQuery>
            <![CDATA[select 'PSFT_GP_EMPLOYEES_RETRO_CALC_DATE_NOT_MATCH' sensor, to_timestamp(to_char(sysdate, 'YYYY-MM-DD hh24:mi:ss'), 'YYYY-MM-DD hh24:mi:ss') entrytime, s.KEY_DATA FROM (Select 'Review ' || count(*) || ' Employee(s) Who Has a Retroactive Calc Begin Date With Unmatched Earliest Job Record Row' AS KEY_DATA, 'PSFT Global Payroll - Employees Whose Retro Calculation Begin Date Does Not Match Earliest JOB Record (Doc ID 2579771.1)' SOLUTION, 'Y' AS EVENT  From(  SELECT DISTINCT  ('Empl ID = ' || A.EMPLID || '; No Retro Processing Before = ' || B.GP_CALC_BGN_DT || '; Payroll System = ' || A.PAY_SYSTEM_FLG || '; Effective Date = ' || A.EFFDT) AS KEY_DATA  FROM SYSADM.PS_JOB A,  SYSADM.PS_GP_PAYEE_DATA B,  SYSADM.PS_PERSONAL_DATA C  WHERE A.EMPLID= B.EMPLID  AND A.EFFDT <> B.GP_CALC_BGN_DT  AND A.EFFDT = (SELECT MIN (C.EFFDT)  FROM SYSADM.PS_JOB C  WHERE A.EMPLID = C.EMPLID  AND C.PAY_SYSTEM_FLG = 'GP')  AND C.EMPLID = B.EMPLID)  having count(*) > 0) s]]>
        </SQLQuery>
