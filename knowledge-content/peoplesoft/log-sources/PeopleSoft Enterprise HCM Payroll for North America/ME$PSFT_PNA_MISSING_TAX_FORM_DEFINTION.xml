    <Patterns>
        <DBPattern dateTimeField="entrytime" dateTimeColumns="entrytime">
        <SQLQuery>
            <![CDATA[select 'PSFT_PNA_MISSING_TAX_FORM_DEFINTION' sensor, to_timestamp(to_char(sysdate, 'YYYY-MM-DD hh24:mi:ss'), 'YYYY-MM-DD hh24:mi:ss') entrytime, s.KEY_DATA FROM (select 'Y' as EVENT  ,'Proposed solution: EPY: (YEMaster) Data Being Added Conflicts With Existing Data (18,2) (Doc ID 658061.1)' AS SOLUTION  ,'Effective Date mismatch found on Year End Tax Form Box information' as KEY_DATA  from (  select distinct  ('Tax form ID = ' || TAXFORM_ID || '; Effective date = ' || EFFDT || '; Box = ' || BOX || '; Description = ' || DESCR || '; Short description = ' || DESCRSHORT ) AS KEY_DATA  From SYSADM.Ps_Taxform_Box  Where Taxform_Id In ('W','A') And  Effdt = Case  When Trunc(Sysdate) Between To_Date('1/NOV/'||To_Char(Sysdate, 'YYYY'), 'DD/MM/YYYY') And To_Date('31/DEC/'||To_Char(Sysdate, 'YYYY'), 'DD/MM/YYYY') Then '1-JAN-'||To_Char(Sysdate, 'YYYY')  When Trunc(Sysdate) Between To_Date('1/JAN/'||To_Char(Sysdate, 'YYYY'), 'DD/MM/YYYY') And To_Date('31/JAN/'||To_Char(Sysdate, 'YYYY'), 'DD/MM/YYYY') Then '1-JAN-'||(To_Char(Sysdate, 'YYYY')-1)  End  )having count(*) >= 1) s]]>
        </SQLQuery>
